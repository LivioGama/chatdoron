{
  "name": "chatdoron",
  "private": true,
  "scripts": {
    "dev-all": "turbo run dev",
    "dev-frontend": "turbo run dev --filter=frontend",
    "dev-backend": "turbo run dev --filter=backend",
    "build": "turbo run build",
    "build-frontend": "turbo run build --filter=frontend",
    "build-backend": "turbo run build --filter=backend",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,md}\"",
    "vercel-pull-env-preview": "npx vercel env pull --environment preview --yes .env",
    "vercel-pull-env-production": "npx vercel env pull --environment production --yes .env",
    "vercel-pull-env-int": "npx vercel env pull --environment preview --git-branch int --yes .env",
    "kill": "lsof -i tcp:3000 -t | xargs -r sudo kill",
    "format:packagejson": "bunx prettier-package-json --write ./package.json",
    "clean-all-node-modules": "bunx npkill",
    "ncu-u": "ncu -u --deep",
    "release-all-ports": "killall node"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@expo-google-fonts/merriweather": "^0.2.3",
    "@expo-google-fonts/open-sans": "^0.2.3",
    "@faker-js/faker": "^8.4.1",
    "@google/generative-ai": "^0.3.0",
    "@legendapp/state": "^3.0.0-beta.3",
    "@react-navigation/native-stack": "^6.11.0",
    "@slack/webhook": "^7.0.2",
    "@tanstack/react-query": "^5.51.11",
    "ai": "^3.0.12",
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "eslint-config-next": "^13.4.1",
    "eslint-plugin-prefer-arrow-functions": "^3.2.4",
    "eslint-plugin-react": "7.28.0",
    "expo": "~51.0.28",
    "expo-font": "^12.0.10",
    "expo-image": "^1.13.0",
    "expo-linear-gradient": "^13.0.2",
    "expo-router": "^3.5.23",
    "lodash": "^4.17.21",
    "models": "workspace:*",
    "openai": "^4.29.0",
    "openai-edge": "^1.2.2",
    "plausible-tracker": "^0.3.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-ficus-ui": "^1.0.0",
    "react-native-gesture-handler": "^2.20.0",
    "react-native-mmkv": "^3.0.2",
    "react-native-reanimated": "^3.15.3",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-web": "^0.19.12",
    "tsconfig": "workspace:*",
    "ui": "workspace:*",
    "use-async-effect": "^2.2.7",
    "zod-class": "^0.0.9"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/react": "18.2.8",
    "prettier": "latest",
    "turbo": "latest",
    "typescript": "^5.4.2"
  },
  "resolutions": {
    "@types/react": "18.2.8"
  },
  "packageManager": "bun@1.1.29"
}
