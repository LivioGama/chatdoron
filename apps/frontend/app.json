{
  "expo": {
    "name": "ChatdOron",
    "slug": "chatdoron",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_plain.png",
    "scheme": "chatdoron",
    "userInterfaceStyle": "automatic",
    "updates": {
      "enabled": false
    },
    "androidStatusBar": {
      "hidden": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A4338"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "ch.chatdoron.chat",
      "buildNumber": "1",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4A4338"
      },
      "package": "ch.chatdoron.chat"
    },
    "plugins": [
      "expo-router"
    ],
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
